{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es6", "dom", "es2019"],
    "importHelpers": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "allowJs": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "useUnknownInCatchVariables": true
  },
  "exclude": [
    "android",
    "ios",
    "node_modules"
  ]
}
