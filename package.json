    "react-native-webview": "8.0.0",
    "react-navigation": "^3.3.0",
    "react-navigation-redux-helpers": "^3.0.0",
    "react-redux": "6.0.0",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "2.1.4",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.2",
    "remark": "10.0.1",
    "remark-custom-blocks": "2.3.1",
    "remark-html": "9.0.0",
    "reselect": "4.0.0",
    "rn-placeholder": "^1.3.3",
    "semver": "^5.7.0",
    "source-map": "^0.6.1",
    "stacktrace-js": "^2.0.0",
    "tslib": "^1.9.3",
    "typesafe-actions": "^4.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.7.0",
    "whatwg-fetch": "git://github.com/github/fetch#141665b1477bb25ac1427b2cb3e05203453e30cb",
    "xss": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@types/chalk": "^2.2.0",
    "@types/color": "^3.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/jest": "23.3.12",
