{
  "name": "italia-app",
  "version": "0.1.0",
  "io_backend_api": "https://raw.githubusercontent.com/pagopa/io-backend/v5.21.1/api_backend.yaml",
  "io_public_api": "https://raw.githubusercontent.com/pagopa/io-backend/v5.21.1/api_public.yaml",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "run-ios": "react-native run-ios",
    "run-android": "react-native run-android",
    "attach": "react-native attach",
    "postinstall": "patch-package && rn-nodeify --install path,buffer && chmod +x ./bin/add-ios-source-maps.sh && ./bin/add-ios-source-maps.sh && chmod +x ./bin/add-ios-env-config.sh && ./bin/add-ios-env-config.sh",
    "test": "jest",
    "prettify": "prettier --write \"ts/**/*.ts*\"",
    "packager:clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "bundle:android": "node node_modules/react-native/local-cli/cli.js bundle --platform android --dev true --entry-file index.js --bundle-output ./bundles/index.android.bundle",
    "tsc:noemit": "tsc --noemit",
    "lint": "tslint -p . -c tslint.json -t verbose",
    "lint-autofix": "tslint -p . -c tslint.json -t verbose --fix",
    "check_urls": "python3 scripts/check_urls.py",
    "generate:pagopa-api-definitions": "rimraf definitions/pagopa && mkdir -p definitions/pagopa && gen-api-models --api-spec https://wisp2.pagopa.gov.it/pp-restapi-CD/v2/api-docs --out-dir ./definitions/pagopa --no-strict --request-types --response-decoders --default-error-type undefined",
    "generate:mock-google-services-json": "cp mock-google-services.json ./android/app/google-services.json",
    "generate:api-definitions": "rimraf definitions/backend && mkdir -p definitions/backend && gen-api-models --api-spec $npm_package_io_backend_api --out-dir ./definitions/backend --no-strict --request-types --response-decoders  && gen-api-models --api-spec $npm_package_io_public_api --out-dir ./definitions/backend",
    "bundle:android-release": "node node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "generate:locales": "ts-node --skip-project -O '{\"lib\":[\"es2015\"]}' scripts/make-locales.ts",
    "generate:content-definitions": "rimraf definitions/content && mkdir -p definitions/content && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-services-metadata/master/definitions.yml --out-dir ./definitions/content",
    "generate:all": "npm-run-all generate:mock-google-services-json generate:api-definitions generate:pagopa-api-definitions generate:content-definitions generate:locales",
    "locales_unused": "ts-node --skip-project -O '{\"lib\":[\"es2015\"]}' scripts/unused-locales.ts"
  },
  "dependencies": {
    "@pagopa/react-native-cie": "0.2.2",
    "@react-native-community/async-storage": "^1.6.1",
    "abort-controller": "^1.0.2",
    "async-mutex": "^0.1.3",
    "buffer": "^4.9.1",
    "color": "^3.0.0",
    "date-fns": "^1.29.0",
    "fp-ts": "1.12.0",
    "hoist-non-react-statics": "^3.0.1",
    "instabug-reactnative": "8.7.0",
    "io-ts": "1.8.5",
    "italia-pagopa-commons": "^1.2.0",
    "italia-ts-commons": "5.1.11",
    "lodash": "^4.17.15",
    "native-base": "2.13.8",
    "native-base-shoutem-theme": "0.3.1",
    "path-browserify": "0.0.0",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-android-open-settings": "^1.3.0",
    "react-native-background-timer": "2.1.1",
    "react-native-barcode-builder": "^1.0.5",
    "react-native-calendar-events": "1.7.1",
    "react-native-camera": "3.4.0",
    "react-native-config": "1.0.0",
    "react-native-device-info": "5.5.3",
    "react-native-exception-handler": "2.10.0",
    "react-native-flag-secure-android": "git://github.com/pagopa/react-native-flag-secure-android.git#7afcf2a418b945f976a4f0e7a72e89301253c795",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-i18n": "^2.0.15",
    "react-native-image-pan-zoom": "^2.1.11",
    "react-native-image-picker": "^0.28.1",
    "react-native-keychain": "^3.1.3",
    "react-native-lewin-qrcode": "^1.0.2",
    "react-native-mixpanel": "1.2.0",
    "react-native-offline": "5.2.0",
    "react-native-popup-menu": "^0.14.0",
    "react-native-progress-circle": "^2.1.0",
