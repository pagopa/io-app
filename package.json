{
  "name": "ItaliaApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "chmod +x ./bin/add-ios-source-maps.sh && ./bin/add-ios-source-maps.sh",
    "test": "jest",
    "prettify": "prettier --write 'js/**/*.js'",
    "lint": "eslint 'js/**/*.js'",
    "packager:clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "bundle:android": "node node_modules/react-native/local-cli/cli.js bundle --platform android --dev true --entry-file index.js --bundle-output ./bundles/index.android.bundle"
  },
  "dependencies": {
    "color": "^2.0.0",
    "email-validator": "^1.0.7",
    "lodash": "^4.17.4",
    "native-base": "^2.3.3",
    "option": "^0.2.3",
    "react": "16.0.0",
    "react-native": "0.50.0",
    "react-native-config": "^0.11.2",
    "react-native-device-info": "^0.13",
    "react-native-fs": "^2.9",
    "react-native-mixpanel": "^0.0.15",
    "react-native-sha256": "^1.0.1",
    "react-navigation": "git+https://github.com/react-community/react-navigation.git",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-persist": "^4.6.0",
    "redux-thunk": "^2.2.0",
    "source-map": "^0.5.6",
    "stacktrace-js": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.2",
    "jest": "^22.0.3",
    "prettier": "1.8.2",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/TitilliumWeb"
    ]
  }
}
